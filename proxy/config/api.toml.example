[http.routers]
    [http.routers.api-router]
        rule = "HostRegexp(`{any:.+}`)"
        entryPoints = ["websecure"]
        service = "api-service"
        [http.routers.api-router.tls]

[http.services]
    [[http.services.api-service.loadBalancer.servers]]
        url = "http://192.168.0.10:8081"
